#version 330 core

// Input vertex data, different for all executions of this shader.
layout(location = 0) in vec4 vertexPosition_modelspace;
layout(location = 1) in vec2 texCoord;

out vec2 v_TexCoord;

uniform mat4 u_mvp;

void main()
{
    gl_Position =  u_mvp * vertexPosition_modelspace;
    v_TexCoord = texCoord;
}

